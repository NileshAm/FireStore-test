{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      [
        {
          "name": "frontend",
          "icon": "browser",
          "color": "terminal.ansiGreen",
          "autoExecuteCommands": true,
          "joinOperator": ";",
          "focus": true,
          "commands": [
            "cd ./frontend",
            "npm run dev"
          ]
        },
        {
          "name": "backend",
          "icon": "server",
          "color": "terminal.ansiGreen",
          "autoExecuteCommands": true,
          "joinOperator": ";",
          "commands": [
            "cd ./backend",
            "npm run dev"
          ]
        }
      ],
      [
        {
          "name": "frontend installer",
          "icon": "browser",
          "autoExecuteCommands": true,
          "commands": [
            "cd ./frontend"
          ]
        },
        {
          "name": "backend installer",
          "icon": "server",
          "autoExecuteCommands": true,
          "commands": [
            "cd ./backend"
          ]
        }
      ]
    ],
    "saved-session": [
      {
        "name": "connect",
        "commands": [
          ""
        ]
      }
    ]
  }
}